<script src= "js/jquery-2.1.1.min.js"></script>
<script src= "js/jsdb.js"></script>

<div class='panel panel-success'>
	<div class='panel-heading'>
		<h3> Database Contents </h3>
		
	</div>
	<div class='panel-body'>
		<form role='form'>
			<div class='form-group'>
				{{#each result}}
					<div><input id={{id}} type='radio' name='input'>
					<label style='margin:10px;'>{{id}}</label></div>
     			{{/each}}
     		</div>
		</form>
     	<button class='btn btn-default pull-right' id='submit' type='button'>Submit</button>
     </div>
</div>
</div>